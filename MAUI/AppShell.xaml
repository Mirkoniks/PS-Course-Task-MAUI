<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUI"
    xmlns:localPages="clr-namespace:MAUI.Windowses"
    Shell.FlyoutBehavior="Disabled"
    Title="MAUI">

    <ShellContent
        ContentTemplate="{DataTemplate localPages:LoginWindow}"
        Route="Login" />
    <ShellContent
        ContentTemplate="{DataTemplate localPages:StudentsList}"
        Route="StudentsList" />

    <ShellContent 
        ContentTemplate="{DataTemplate localPages:AddUser}"
        Route="AddUser" />

    <ShellContent 
    ContentTemplate="{DataTemplate localPages:RemoveUser}"
    Route="RemoveUser" />

    <ShellContent 
    ContentTemplate="{DataTemplate localPages:Logs}"
    Route="Logs" />


</Shell>
